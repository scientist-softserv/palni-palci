<td><strong><%= t(".label", type: type.humanize) %></strong></td>
<td>
  <% if items.blank? %>
    <%= t('.empty', type: type.humanize) %>
  <% else %>
    <ul class ="tabular">
      <% items.each do |item| %>
        <li class='attribute attribute-title'>
          <%= link_to item.title.first, url_for_document(item) %>
        </li>
      <% end %>
    </ul>
  <% end %>
</td>
